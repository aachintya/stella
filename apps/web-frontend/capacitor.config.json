{
    "appId": "com.stellariumlabs.stellaweb",
    "appName": "VayuView",
    "webDir": "dist",
    "bundledWebRuntime": false,
    "server": {
        "androidScheme": "https"
    },
    "android": {
        "allowMixedContent": true,
        "webContentsDebuggingEnabled": false,
        "adjustMarginsForEdgeToEdge": "force"
    },
    "plugins": {
        "SystemBars": {
            "style": "DARK",
            "hidden": true,
            "insetsHandling": "css"
        }
    }
}